<br />
<div class="container row p-0 m-0">
    <div class="col-6">
        <h2 class="text-success">Project List</h2>
    </div>
    <div class="col-3 offset-3">
        <a asp-action="Upsert" asp-controller="Project" class="btn btn-success form-control text-white">
            Add New Project
        </a>
    </div>
    <div class="col-12 border p-3">
        <table id="DT-load" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Project Manager</th>
                    <th>Project Number</th>
                    <th></th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Scripts{
    <script src="~/js/ProjectList.js"></script>
    }